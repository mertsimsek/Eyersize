<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.fourapps.eyetraining">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light">
        <activity
            android:name="co.fourapps.eyetraining.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="co.fourapps.eyetraining.RoutineOneActivity"
            android:label="@string/title_activity_astigmat"></activity>
        <activity
            android:name="co.fourapps.eyetraining.ExerciseDetail"
            android:label="@string/title_activity_exercise_detail"></activity>
    </application>

</manifest>
